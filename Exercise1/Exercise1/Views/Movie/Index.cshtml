@model IEnumerable <Exercise1.Models.Movie>
@{
    ViewBag.Title = "Movie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Movies</h2>

<table>
    <tr>
        <th>
            Movie Name
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.ActionLink(item.Name,"MovieSpecificView",new { Id=item.Id })</td>
        </tr>
    }
</table>

